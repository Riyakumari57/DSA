#include<bits/stdc++.h>
using namespace std;
int main()
{
    int test;
    cin>>test;
    while(test--)
    {
        int n;
        cin>>n;
        if(n<=0) cout<<"false";
        int x = n&(n-1);
        if(x==0)
        {

           cout<<"true";
        } 
        else
        {
            
             cout<<"false";
        }
            
    }
    

}
